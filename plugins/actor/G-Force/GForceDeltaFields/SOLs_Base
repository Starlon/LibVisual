//Sound of Lights Base
//Interesting foreshortening, like driving really fast!

Aspc=0,

A0="7",
A1="1.05 / (A0 + 1)",

srcX="x * ((y + A0) * A1)",
srcY="y - (y-1.2) * .03",

Vers=100