name="Flower Power",
// Created by Thomas Geier
//ziggystar@gmx.de
//Flower Power

//I know, this (or something similar) already exists(this is in a different velocity)

Aspc=1,

A0="2+trnc(rnd(10))",
A1="rnd(0.06)-0.03",

srcR="r+A1*sin(theta*A0 + 1.571)", // 1.571 == PI / 2
srcT="theta+A1*sin(theta*A0)",

Vers=100