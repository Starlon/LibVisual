 B7=".7 * cos( b1 )",
 B8=".7 * sin( b1 )",

 C0=" b7 * ( s - .1 ) + a0 ",    //  x'
 C1=" b8 * ( s - .1 ) + a0 ",    // y'

 C2=" .7 * ( s - .1 ) * sin( b3 ) + a1 ",  //  x'
 C3=" .7 * ( s - .1 ) * cos( b3 ) + a1 ",  // y'

 C4=" b7 * ( s - .1 ) + a2 ",  //  x'
 C5=" b8 * ( s - .1 ) + a2 ",  // y'

 C6=" b7 * ( s - .1 ) + a3 ",  //  x'
 C7=" b8 * ( s - .1 ) + a3 ",  // y'

