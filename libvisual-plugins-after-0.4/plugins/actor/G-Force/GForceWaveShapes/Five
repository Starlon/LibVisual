 A3=" sin( .2 + rnd( .7 ) ) ",  // orbit radius
 A4=" cos( .6 + rnd( .3 ) ) ",  // orbit radius

 B0=" -1.0 * t ",               // theta from (0,0)
 B1=" .3 * t ",                  // theta from center of rod

