name="DT - Horizon",
  /* config for G-Force Winamp, SoundJam & Macast visual plugin
         made by David Tryse TomteSkogen42@hotmail.com */
 Aspc=1,
 
  //A0=shrink, A1=horizon, A2=speed
 A0="1.03 + rnd(0.02)",
 A1="rnd(2)-0.5",
 A2="0.015 + (rnd(0.07) * rnd(1))*(1+A1)",
 
 srcX="x * A0",
 srcY="y + A2 * (A1 + y -1)",
 
