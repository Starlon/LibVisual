name="Quad-Circle",


Aspc=0,

Stps="100",



C0="mag( s ) * 0.35 + .4",  // radius
C1="sin( s * 1.57079632 )",
C2="cos( s * 1.57079632 )",
C3=".9 + .3 * mag( .7 * s )",


X0="c3 * c2 - 1",
Y0="c3 * c1 - 1",

X1="c3 * c2 - 1",
Y1="1 - c3 * c1",

X2="1 - c3 * c2",
Y2="1 - c3 * c1",

X3="1 - c3 * c2",
Y3="c3 * c1 - 1",

ConB=1,
LWdt="1",

Vers=100